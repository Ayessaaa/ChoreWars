              <p class="text-slate-400 text-sm -mt-0.5">Chore of: <% for (let i = 0; i < chore.participants.length; i++) {
                      const participant = chore.participants[i]; %>
                      <%= participant %><%= i+1<chore.participants.length ? "," : "" %>
                      
                    <% } %></p>
              <div class="flex gap-1 text-red-500/80 mt-2">
                <div class="bg-red-500/20 text-xs font-medium px-3 py-0.5 rounded-full capitalize">
                  <%= chore.frequency %>
                </div>
              </div>
            </div>

            <div class="flex flex-col items-end gap-3">
              <div class="flex flex-row gap-1">
                
                <button title="edit" onclick="window.location.href = '/edit-chore/<%= chore._id %>'">
                  <svg xmlns="http://www.w3.org/2000/svg" 
                      id="Layer_1" 
                      data-name="Layer 1" 
                      viewBox="0 0 24 24"
                      class="size-5 fill-yellow-500 drop-shadow-[0_5px_5px_rgba(234,179,8,.3)] hover:scale-105 transition-all active:fill-yellow-600"
                      >
                    <path d="M22.987,5.452c-.028-.177-.312-1.767-1.464-2.928-1.157-1.132-2.753-1.412-2.931-1.44-.237-.039-.479,.011-.682,.137-.071,.044-1.114,.697-3.173,2.438,1.059,.374,2.428,1.023,3.538,2.109,1.114,1.09,1.78,2.431,2.162,3.471,1.72-2.01,2.367-3.028,2.41-3.098,.128-.205,.178-.45,.14-.689Z"/>
                    <path d="M12.95,5.223c-1.073,.968-2.322,2.144-3.752,3.564C3.135,14.807,1.545,17.214,1.48,17.313c-.091,.14-.146,.301-.159,.467l-.319,4.071c-.022,.292,.083,.578,.29,.785,.188,.188,.443,.293,.708,.293,.025,0,.051,0,.077-.003l4.101-.316c.165-.013,.324-.066,.463-.155,.1-.064,2.523-1.643,8.585-7.662,1.462-1.452,2.668-2.716,3.655-3.797-.151-.649-.678-2.501-2.005-3.798-1.346-1.317-3.283-1.833-3.927-1.975Z"/>
                  </svg>
                </button>
                <button title="delete" onclick="deleteFunc('<%= chore._id %>')">
                  <svg xmlns="http://www.w3.org/2000/svg"
                      xmlns:xlink="http://www.w3.org/1999/xlink" 
                      version="1.1" 
                      id="Capa_1"
                      viewBox="0 0 512 512"
                      xml:space="preserve"
                      class="size-5 fill-red-500 drop-shadow-[0_5px_5px_rgba(239,68,68,.3)] hover:scale-105 transition-all active:fill-red-600"
                      >
                    <g>
                      <path d="M448,85.333h-66.133C371.66,35.703,328.002,0.064,277.333,0h-42.667c-50.669,0.064-94.327,35.703-104.533,85.333H64   c-11.782,0-21.333,9.551-21.333,21.333S52.218,128,64,128h21.333v277.333C85.404,464.214,133.119,511.93,192,512h128   c58.881-0.07,106.596-47.786,106.667-106.667V128H448c11.782,0,21.333-9.551,21.333-21.333S459.782,85.333,448,85.333z    M234.667,362.667c0,11.782-9.551,21.333-21.333,21.333C201.551,384,192,374.449,192,362.667v-128   c0-11.782,9.551-21.333,21.333-21.333c11.782,0,21.333,9.551,21.333,21.333V362.667z M320,362.667   c0,11.782-9.551,21.333-21.333,21.333c-11.782,0-21.333-9.551-21.333-21.333v-128c0-11.782,9.551-21.333,21.333-21.333   c11.782,0,21.333,9.551,21.333,21.333V362.667z M174.315,85.333c9.074-25.551,33.238-42.634,60.352-42.667h42.667   c27.114,0.033,51.278,17.116,60.352,42.667H174.315z"/>
                    </g>
                  </svg>
                </button>
              </div>
              <p class="text-slate-200 text-xl font-bold mt-1 bg-yellow-500/10 px-2 rounded-lg"><%= chore.points %> <span class="text-sm font-semibold text-yellow-500/80">pts</span></p>

            </div>

          </div>
        <% } %>

        
        <script>
          function deleteFunc(id) {
            if(confirm("Delete this chore?")){
              
              window.location.href = `/delete-chore/${id}`}
            }
          </script>
        
      </div>
    </div>
  </body>
</html>
